   "subtotal":{
      "name":"subtotal",
      "vname":"LBL_SUBTOTAL",
      "type":"currency",
      "len":null,
      "default":"0",
      "related_fields":[
         "currency_id",
         "base_rate",
         "discount_price",
         "quantity"
      ],
      "formula":"\n                 ifElse(and(isNumeric($quantity), isNumeric($discount_price)),\n                   ifElse(equal($quantity, 0), \"0\", currencyMultiply($discount_price, $quantity))\n                 , \"\")",
      "calculated":true,
      "enforced":true,
      "massupdate":false
   },
   "total_amount":{
      "name":"total_amount",
      "default":"0.00",
      "formula":"\n                ifElse(and(isNumeric($quantity), isNumeric($discount_price)),\n                    currencySubtract(\n                        currencyMultiply(\n                            $discount_price,\n                            $quantity\n                        ),\n                        ifElse(equal($discount_select, \"1\"),\n                            currencyMultiply(currencyMultiply($discount_price, $quantity), currencyDivide($discount_amount, 100)),\n                            ifElse(isNumeric($discount_amount), $discount_amount, 0)\n                        )\n                    ),\n                    \"\"\n                )",
      "calculated":true,
      "enforced":true,
      "vname":"LBL_CALCULATED_LINE_ITEM_AMOUNT",
      "reportable":false,
      "type":"currency",
      "related_fields":[
         "currency_id",
         "base_rate",
         "quantity",
         "discount_price",
         "discount_select",
         "discount_amount"
      ],
      "massupdate":false
   },
